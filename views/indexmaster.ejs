function annualBill(init, end) {

      var init;
      var end;
      //
    for (i=init; i<end; i++) {

    document.getElementById('sec' + i + 'row1col13').value = 
    (unnan(document.getElementById('sec' + i + 'row1col1').value)) +
    (unnan(document.getElementById('sec' + i + 'row1col2').value)) +
