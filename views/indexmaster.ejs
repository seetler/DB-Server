    (unnan(document.getElementById('sec' + i + 'row1col11').value)) +
    (unnan(document.getElementById('sec' + i + 'row1col12').value)) ;
    $('#sec' + i + 'row1col13').autoNumeric('update', {aSign: '$ '});

    }
      //
}

function annualHoldout(init, end) {
      //
      var init;
      var end;
      //
    for (i=init; i<end; i++) {

    var y= 
    (unnan(document.getElementById('sec' + i + 'row2col1').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col1').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col2').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col2').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col3').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col3').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col4').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col4').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col5').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col5').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col6').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col6').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col7').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col7').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col8').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col8').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col9').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col9').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col10').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col10').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col11').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col11').value)) +
    (unnan(document.getElementById('sec' + i + 'row2col12').value)/100)* (unnan(document.getElementById('sec' + i + 'row1col12').value)) ;

    var x=y/(unnan(document.getElementById('sec' + i + 'row1col13').value))*100

    document.getElementById('sec' + i + 'row2col13').value=x;

    $('#sec' + i + 'row2col13').autoNumeric('update', {aSign: ' %', pSign:'S'});
    }
      //
}





