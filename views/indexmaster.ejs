$(document).on('keydown', 'input', function(e) {

    var value = this.name;
    var rowvalue = value.substring(value.indexOf('row')+3,value.indexOf('col'));
    var secvalue = value.substring(value.indexOf('sec')+3,value.indexOf('row'));
    var firstpart = value.substring(0,value.indexOf('row')+3);
    var secondpart = value.substring(value.indexOf('col'));
    var rowvaluedown = Number(rowvalue)+1;
    var rowvalueup = Number(rowvalue)-1;
    var colvalue = value.substring(value.indexOf('col')+3);
    var colvalueup = Number(colvalue)+1
    var colvaluedown = Number(colvalue)-1


    if(e.keyCode == 40) {

        if (rowvaluedown == 3) {
          rowvaluedown = 1;
          secvalue = Number(secvalue)+1
        }

        var finalvalue = '#sec'+secvalue+'row'+rowvaluedown+secondpart
          $(finalvalue).focus();
          e.preventDefault();

    };



    if(e.keyCode == 39) {

        if (colvalueup == 13 || colvalueup == 14) {
          colvalueup = 1;
          rowvalue = Number(rowvalue)+1

          if (rowvalue == 3) {
            rowvalue = 1;
            secvalue = Number(secvalue)+1
          }
        }

        var finalvalue = '#sec'+secvalue+'row'+rowvalue+'col' + colvalueup
          $(finalvalue).focus();
          e.preventDefault();

    };




    if(e.keyCode == 37) {

        if (colvaluedown == 0) {
          colvaluedown = 12;
          rowvalue = Number(rowvalue)-1

          if (rowvalue == 0) {
            rowvalue = 2;
            secvalue = Number(secvalue)-1
          }
        }

        var finalvalue = '#sec'+secvalue+'row'+rowvalue+'col' + colvaluedown
          $(finalvalue).focus();
          e.preventDefault();
    };


    if(e.keyCode == 38) { 

        if (rowvalueup == 0) {
          rowvalueup = 2;
          secvalue = Number(secvalue)-1
        }

        var finalvalue = '#sec'+secvalue+'row'+rowvalueup+secondpart
          $(finalvalue).focus()
          e.preventDefault();
        };


});
